{
  "name": "materialthemechangelog",
  "private": true,
  "version": "1.0.0",
  "description": "Material Theme Changelog on Sanity.io",
  "main": "package.json",
  "license": "Apache 2.0",
  "scripts": {
    "start": "sanity start",
    "deploy": "sanity deploy",
    "test": "xo && sanity check",
    "test-travis": "xo"
  },
  "keywords": [
    "sanity"
  ],
  "dependencies": {
    "@sanity/base": "^0.139.1",
    "@sanity/components": "^0.139.1",
    "@sanity/core": "^0.139.1",
    "@sanity/default-layout": "^0.139.1",
    "@sanity/default-login": "^0.139.0",
    "@sanity/desk-tool": "^0.139.1",
    "prop-types": "^15.6",
    "react": "^16.2",
    "react-dom": "^16.2"
  },
  "devDependencies": {
    "xo": "0.23.0"
  },
  "xo": {
    "spaces": true,
    "rules": {
      "max-len": [
        "error",
        {
          "code": 120,
          "ignoreUrls": true
        }
      ],
      "capitalized-comments": "off"
    }
  }
}
